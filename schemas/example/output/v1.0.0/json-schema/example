<span class="w">                                </span><span class="nt">&quot;name&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;Ti&quot;</span><span class="p">,</span><span class="w"></span>
<span class="w">                                </span><span class="nt">&quot;chemical_symbols&quot;</span><span class="p">:</span><span class="w"> </span><span class="p">[</span><span class="w"></span>
<span class="w">                                    </span><span class="s2">&quot;Ti&quot;</span><span class="w"></span>
<span class="w">                                </span><span class="p">],</span><span class="w"></span>
<span class="w">                                </span><span class="nt">&quot;concentration&quot;</span><span class="p">:</span><span class="w"> </span><span class="p">[</span><span class="w"></span>
<span class="w">                                    </span><span class="mf">1.0</span><span class="w"></span>
<span class="w">                                </span><span class="p">]</span><span class="w"></span>
<span class="w">                            </span><span class="p">}</span><span class="w"></span>
