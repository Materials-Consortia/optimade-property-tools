<span class="w">                            </span><span class="s2">&quot;Ti&quot;</span><span class="p">,</span><span class="w"></span>
<span class="w">                            </span><span class="s2">&quot;O2&quot;</span><span class="w"></span>
<span class="w">                        </span><span class="p">],</span><span class="w"></span>
<span class="w">                        </span><span class="p">[</span><span class="w"></span>
