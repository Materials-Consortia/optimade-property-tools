<span class="w">                            </span><span class="nt">&quot;response-level&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;may&quot;</span><span class="p">,</span><span class="w"></span>
<span class="w">                            </span><span class="nt">&quot;query-support&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;all mandatory&quot;</span><span class="w"></span>
<span class="w">                        </span><span class="p">},</span><span class="w"></span>
<span class="w">                        </span><span class="nt">&quot;title&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;Number of periodic dimensions&quot;</span><span class="p">,</span><span class="w"></span>
<span class="w">                        </span><span class="nt">&quot;x-optimade-type&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;integer&quot;</span><span class="p">,</span><span class="w"></span>
